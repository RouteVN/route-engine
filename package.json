{
  "name": "route-engine-js",
  "version": "0.0.2-rc13",
  "type": "module",
  "main": "dist/RouteEngine.js",
  "exports": {
    ".": "./dist/RouteEngine.js"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "bun run esbuild.js",
    "lint": "bunx prettier src -c",
    "lint:fix": "bunx prettier src -w",
    "coverage": "vitest run --coverage",
    "vt:generate": "bun run esbuild.js && rtgl vt generate --skip-screenshots",
    "serve": "bun run esbuild.js && bunx serve -p 3004 .rettangoli/vt/_site"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "jempl": "0.2.0-rc1",
    "js-yaml": "^4.1.0",
    "puty": "^0.0.4-rc2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "vitest": "^3.2.4"
  }
}
